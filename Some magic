#include <SPI.h>

#define redLed 5
#define blueLed 6
TMRpcm tmrpcm;    

 int b1, b2, b3;
 int light;


  


void setup() {
   
  
  Serial.begin(9600);
  

}

void loop() {
  
  light = analogRead(0);
  delay(100);
  Serial.println(light);
  delay(1000);
}
